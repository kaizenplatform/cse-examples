{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "chrome": "78",
          "firefox": "72",
          "ie": "11",
          "edge": "17",
          "safari": "11"
        },
        "useBuiltIns": "usage",
        "corejs": "3.6"
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      { "corejs": 3, "useESModules": true, "version": "^7.10.1" }
    ]
  ]
}
